---
import "../styles/style.scss";
import SectionTitle from "../components/SectionTitle.astro";
import LessonTitle from "../components/LessonTitle.astro";
import ButtonCta from "../components/ButtonCta.astro";
import ContactSection from "../components/ContactSection.astro";

// import Layout from '../layouts/Layout.astro';

const logoWide = "/assets/images/global/logo-wide.svg";
const logoTall = "/assets/images/global/logo-tall.svg";

const navList = [
  {
    href: '/#concept',
    label: 'コンセプト'
  },
  {
    href: '/#menu',
    label: 'メニュー'
  },
  {
    href: '/#access',
    label: 'アクセス'
  },
  {
    href: '/#faq',
    label: 'よくある質問'
  },
  {
    href: '/contact',
    label: 'お問い合わせ'
  }
]

const snsList = [
  {
    href: 'https://line.me/R/ti/p/@309upsyu?ts=03010941&oat_content=url',
    src: '/assets/images/global/icon-line.svg',
    ariaLabel: 'ラインで友だち追加'
  },
  {
    href: 'https://www.instagram.com/kotono_yoga',
    src: '/assets/images/global/icon-instagram.svg',
    ariaLabel: 'インスタグラムを見る'
  }
]

const {
  subTitle = "トップページ",
  ogType = "website",
  ogTitle = subTitle,
  description = "埼玉県羽生市のプライベートサロン「mahana」では、薬剤師オーナーが腸律セラピー・アロマトリートメント・よもぎ蒸し・フェイシャルエステをご提供。腸から美しく整え、心と身体のバランスを整える癒しの時間をお届けします。",
  ogDescription = description,
  // noContact = false,
  noIndex = false,
  jsonLd = ""
} = Astro.props;
---

<!DOCTYPE html>
<html lang="ja" prefix="og: https://ogp.me/ns#">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>埼玉県羽生市の腸律セラピー・アロマ・よもぎ蒸し｜mahana｜{subTitle}</title>
    <meta name="description" content={description}>

    <!-- noindex -->
    <!-- { noIndex && <meta name="robots" content="noindex, nofollow"> } -->
    <meta name="robots" content="noindex, nofollow">

    <!-- favicon -->
    <link rel="shortcut icon" href="/favicon.ico">
    <link rel="apple-touch-icon" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" href="/android-chrome-256x256.png">

    <!-- ogp -->
    <meta property="og:site_name" content="埼玉県羽生市の腸律セラピー・アロマ・よもぎ蒸し｜mahana">
    <meta property="og:url" content={`https://mahana-organic.com${Astro.url.pathname}`} />
    <meta property="og:type" content={ogType}>
    <meta property="og:title" content={ogTitle}>
    <meta property="og:description" content={ogDescription}>
    <meta property="og:image" content="https://mahana-organic.com/ogp.webp">
    <meta property="og:locale" content="ja_JP">
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:description" content={ogDescription}>
    <meta name="twitter:image:src" content="https://mahana-organic.com/ogp.webp">

    <!-- google fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Shippori+Mincho:wght@400&display=swap" rel="stylesheet">

    <!-- destyle.css -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/destyle.css@4.0.0/destyle.min.css">
  </head>

  <body>
    <!-- header -->
    <header class="header" id="site-header">
      <div class="header-container l-container">
        <h1 class="header-logo">
          <a href="/">
            <img src={logoWide} alt="コヨミ ヨガ ピラティス" loading="eager" decoding="async" width="142" height="45">
          </a>
        </h1>
        <a href="https://coubic.com/yogapilates-koyomi" target="_blank" class="header-sp-reservation">ご予約</a>
        <button class="header-hamburger" type="button">
          <span class="header-hamburger-icon"></span>
          <span class="header-hamburger-icon"></span>
          <span class="header-hamburger-icon"></span>
          <span class="u-sr-only">メニュー</span>
        </button>
        <div class="header-link">
          <nav class="header-nav">
            <ul class="header-nav-list">
              <li class="header-nav-item header-nav-item--top">
                <a href="/">トップ</a>
              </li>
              {navList.map((item) =>
                <li class="header-nav-item">
                  <a href={item.href}>{item.label}</a>
                </li>
              )}
            </ul>
          </nav>
          <div class="header-external-link">
            <a href="https://coubic.com/yogapilates-koyomi" target="_blank" class="header-reservation header-reservation--pc">ご予約</a>
            <a href="https://coubic.com/yogapilates-koyomi" target="_blank" class="header-reservation header-reservation--sp">今すぐ予約する</a>
            <ul class="header-sns">
              <!-- {snsList.map((item) =>
                <li class="header-sns-item">
                  <a href={item.href} target="_blank" aria-label={item.ariaLabel}>
                    <img
                      src={item.src}
                      alt=""
                      loading="eager"
                      decoding="async"
                      width="20"
                      height="20"
                    >
                  </a>
                </li>
              )} -->
              <li class="header-sns-item">
                <a href="https://line.me/R/ti/p/@309upsyu?ts=03010941&oat_content=url" target="_blank" aria-label="ラインで友だち追加">
                  <img src="/assets/images/global/icon-line.svg" alt="" loading="eager" decoding="async" width="20" height="20">
                </a>
              </li>
              <li class="header-sns-item">
                <a href="https://www.instagram.com/kotono_yoga" target="_blank" aria-label="インスタグラムを見る">
                  <img src="/assets/images/global/icon-instagram.svg" alt="" loading="eager" decoding="async" width="20" height="20">
                </a>
              </li>
            </ul>
          </div><!-- /.header-external-link -->
        </div><!-- /.header-link -->
      </div><!-- /.header-container.l-container -->
    </header>

    <!-- top-kv -->
    <div class="top-kv">
      <div class="top-kv-bg">
        <picture>
          <source media="(max-width: 767px)" srcset="/assets/images/kv/pic-top-kv-bg-01-sp@2x.png">
          <source media="(min-width: 768px)" srcset="/assets/images/kv/pic-top-kv-bg-01-pc@2x.png">
          <img src="/assets/images/kv/pic-top-kv-bg-01-sp@2x.png" alt="" loading="eager" decoding="async" width="375" height="619">
        </picture>
      </div><!-- /.top-kv-bg -->
      <div class="top-kv-container l-container">
        <div class="top-kv-logo">
          <img src={logoTall} alt="コヨミ ヨガ ピラティスのロゴ" loading="eager" decoding="async" width="292" height="330">
        </div><!-- /.top-kv-logo -->
        <div class="top-kv-cta">
          <a href="https://coubic.com/yogapilates-koyomi" target="_blank">予約は<br>こちらから<span class="top-kv-cta-arrow"></span></a>
        </div><!-- /.top-kv-cta -->
      </div><!-- /.top-kv-container.l-container -->
    </div><!-- /.top-kv -->

    <main class="top-main">
      <!-- conceptセクションbg設定 ここから -->
      <div class="top-concept-bg">
        <!-- top-catchphrase -->
        <div class="top-catchphrase">
          <div class="top-catchphrase-container l-container-s">
            <p class="top-catchphrase-text"><span class="top-catchphrase-text-lg">心地</span>よい<span class="top-catchphrase-text-lg">毎日</span>をここから。<br>忙しい日常に、<span class="top-catchphrase-text-lg">心</span>と<span class="top-catchphrase-text-lg">体</span>を<span class="top-catchphrase-text-lg">解く時間</span>を。</p>
            <div class="top-catchphrase-image">
              <picture>
                <source media="(max-width: 767px)" srcset="/assets/images/catchphrase/pic-catchphrase-flower-sp@2x.jpg">
                <source media="(min-width: 768px)" srcset="/assets/images/catchphrase/pic-catchphrase-flower-pc@2x.jpg">
                <img src="/assets/images/catchphrase/pic-catchphrase-flower-sp@2x.jpg" alt="" loading="lazy" decoding="async" width="240" height="160">
              </picture>
            </div><!-- /.top-catchphrase-image -->
          </div><!-- /.top-catchphrase-container.l-container-s -->
        </div><!-- /.top-catchphrase -->

        <!-- top-concept -->
        <section class="top-concept" id="concept">
          <div class="l-container-s">
            <div class="top-concept-title">
              <SectionTitle titleText="コンセプト" />
            </div><!-- /.top-concept-title -->
            <div class="top-concept-section-body">
              <div class="top-concept-description">
                <p class="top-concept-text">月日と共に変わる心と身体。<br>歳を重ねることは日々の中で<br class="u-no-pc">自分を見つめ直す旅です。</p>
                <p class="top-concept-text">ここは、がんばる女性たちが<br class="u-no-pc">ほっと一息つける場所。</p>
                <p class="top-concept-text">忙しさに埋もれた心をそっとほどき、<br>あなた自身を大切にする時間を<br class="u-no-pc">お届けします。</p>
                <p class="top-concept-text">ヨガで心を整え、<br class="u-no-pc">内側から軽くなる感覚を。<br>ピラティスで体を支え、<br class="u-no-pc">日々にゆとりをつくる。</p>
                <p class="top-concept-text">季節の移ろいや、<br class="u-no-pc">月のリズムとともに、<br>誰もが安心して自分を取り戻せる、<br class="u-no-pc">そんな居場所を目指しています。</p>
              </div><!-- /.top-concept-description -->
            </div><!-- /.top-concept-section-body -->
          </div><!-- /.l-container-s -->
        </section><!-- /.top-concept -->

        <!-- conceptセクションbg -->
        <div class="top-concept-bg-image"></div>
      </div><!-- /.top-concept-bg -->
      <!-- conceptセクションbg設定 ここまで -->

      <!-- top-programs -->
      <section class="top-programs">
        <div class="top-programs-title">
          <h2 class="top-programs-title-text">女性の体のサイクルにあわせた<br class="u-no-pc">多彩なプログラム</h2>
          <div class="top-programs-title-dec">
            <picture>
              <source media="(max-width: 767px)" srcset="/assets/images/programs/image-programs-title-dec-sp@2x.png">
              <source media="(min-width: 768px)" srcset="/assets/images/programs/image-programs-title-dec-pc@2x.png">
              <img src="/assets/images/programs/image-programs-title-dec-sp@2x.png" alt="" loading="lazy" decoding="async" width="250" height="11">
            </picture>
          </div><!-- /.top-programs-title-dec -->
        </div><!-- /.top-programs-title -->
        <div class="top-programs-section-body l-section-body">
          <ul class="top-programs-list">
            <li class="top-programs-item top-programs-item--01">
              <dl class="top-programs-def-list">
                <dt id="label-ninshin">妊娠期</dt>
                <dd aria-labelledby="label-ninshin">
                  マタニティヨガ
                  <small class="top-programs-note">※妊娠16週目以降</small>
                  <small class="top-programs-note">※医師の許可が必要</small>
                </dd>
              </dl>
            </li>
            <li class="top-programs-item top-programs-item--02">
              <dl class="top-programs-def-list">
                <dt id="label-sango">産後ケア</dt>
                <dd aria-labelledby="label-sango">プライベート<br>マシンピラティス</dd>
              </dl>
            </li>
            <li class="top-programs-item top-programs-item--03">
              <dl class="top-programs-def-list">
                <dt id="label-fullmoon-newmoon">満月〜新月</dt>
                <dd aria-labelledby="label-fullmoon-newmoon">デトックスヨガ</dd>
              </dl>
            </li>
            <li class="top-programs-item top-programs-item--04">
              <dl class="top-programs-def-list">
                <dt id="label-stresscare">ストレスケア</dt>
                <dd aria-labelledby="label-stresscare">モーニングヨガ</dd>
                <dd aria-labelledby="label-stresscare">ストレス改善ヨガ</dd>
                <dd aria-labelledby="label-stresscare">心を満たすヨガ</dd>
              </dl>
            </li>
            <li class="top-programs-item top-programs-item--05">
              <dl class="top-programs-def-list">
                <dt id="label-bodymake">ボディメイク</dt>
                <dd aria-labelledby="label-bodymake">姿勢改善ピラティス</dd>
                <dd aria-labelledby="label-bodymake">くびれフローヨガ</dd>
                <dd aria-labelledby="label-bodymake">季節の太陽礼拝</dd>
              </dl>
            </li>
          </ul>
          <div class="top-programs-images-sp l-container">
            <div class="top-programs-images-sp-upper">
              <img src="/assets/images/programs/pic-programs-sp-01@2x.jpg" alt="" loading="lazy" decoding="async" width="100" height="100">
              <img src="/assets/images/programs/pic-programs-sp-02@2x.jpg" alt="" loading="lazy" decoding="async" width="100" height="100">
              <img src="/assets/images/programs/pic-programs-sp-03@2x.jpg" alt="" loading="lazy" decoding="async" width="100" height="100">
            </div><!-- /.top-programs-images-sp-upper -->
            <div class="top-programs-images-sp-lower">
              <img src="/assets/images/programs/pic-programs-sp-04@2x.jpg" alt="" loading="lazy" decoding="async" width="155" height="155">
              <img src="/assets/images/programs/pic-programs-sp-05@2x.jpg" alt="" loading="lazy" decoding="async" width="155" height="155">
            </div><!-- /.top-programs-images-sp-lower -->
          </div><!-- /.top-programs-images-sp -->
          <div class="top-programs-images-pc l-container">
            <img src="/assets/images/programs/pic-programs-pc-01@2x.jpg" alt="" loading="lazy" decoding="async" width="226" height="226">
            <img src="/assets/images/programs/pic-programs-pc-02@2x.jpg" alt="" loading="lazy" decoding="async" width="226" height="226">
            <img src="/assets/images/programs/pic-programs-pc-03@2x.jpg" alt="" loading="lazy" decoding="async" width="226" height="226">
            <img src="/assets/images/programs/pic-programs-pc-04@2x.jpg" alt="" loading="lazy" decoding="async" width="226" height="226">
            <img src="/assets/images/programs/pic-programs-pc-05@2x.jpg" alt="" loading="lazy" decoding="async" width="226" height="226">
          </div><!-- /.top-programs-images-pc -->
        </div><!-- /.top-programs-section-body.l-section-body -->
      </section><!-- /.top-programs -->

      <!-- top-menu -->
      <section class="top-menu" id="menu">
        <div class="l-container-s">
          <div class="top-menu-title">
            <SectionTitle titleText="メニュー" />
          </div><!-- /.top-menu-title -->
          <div class="top-menu-section-body l-section-body">
            <ul class="top-menu-list">
              <li class="top-menu-item">
                <div class="top-menu-lesson-title">
                  <LessonTitle
                  titleText="グループレッスン"
                  titleTextClass="c-lesson-title-text"
                  imgSrc="/assets/images/global/text-group-lesson.svg"
                  imgWidth="277"
                  imgHeight="100"
                  imgClass="c-lesson-title-image c-lesson-title-image--01"
                  />
                </div><!-- /.top-menu-lesson-title -->
                <div class="top-menu-item-header">
                  <div class="top-menu-description">
                    <p class="top-menu-text">同じ想いを持つ仲間と一緒に、<br>心と体をゆっくりほどく時間を過ごしませんか?</p>
                    <p class="top-menu-text">少人数制だから、慌ただしさはありません。</p>
                    <p class="top-menu-text">落ち着いた空間で、 "誰かと一緒"の安心感が、<br>心の余裕と体の調和を<br>そっと育ててくれます。</p>
                  </div><!-- /.top-menu-description -->
                  <div class="top-menu-image">
                    <img src="/assets/images/menu/pic-menu-group@2x.jpg" alt="" loading="lazy" decoding="async" width="570" height="330">
                  </div><!-- /.top-menu-image -->
                </div><!-- /.top-menu-item-header -->
                <div class="top-menu-item-body">
                  <div class="top-menu-detail">
                    <h4 class="top-menu-detail-heading">単発で通いたい方</h4>
                    <div class="top-menu-one-off">
                      <dl class="top-menu-one-off-def-list">
                        <dt>カウンセリング<span class="top-menu-first-time">(初回のみ)</span><span class="top-menu-plus">＋</span>60分レッスン</dt>
                        <dd>
                          <span aria-label="さんぜんごひゃくえん">
                            <span class="top-menu-price">3,500</span>
                            <span class="top-menu-yen">円</span>
                          </span>
                        </dd>
                      </dl>
                    </div><!-- /.top-menu-one-off -->
                  </div><!-- /.top-menu-detail -->
                  <div class="top-menu-detail">
                    <h4 class="top-menu-detail-heading top-menu-detail-heading--bg-beige">継続して通いたい方</h4>
                    <div class="top-menu-continuation">
                      <p class="top-menu-coupon-ticket">回数券</p>
                      <dl class="top-menu-continuation-def-list">
                        <dt>4回券</dt>
                        <dd>
                          <span aria-label="いちまんにせんはっぴゃくえん">
                            <span class="top-menu-price">12,800</span>
                            <span class="top-menu-yen">円</span>
                          </span>
                          <span class="top-menu-unit-price">(1回あたり<span aria-label="さんぜんにひゃくえん">3,200円</span>)</span>
                        </dd>
                        <dt>8回券</dt>
                        <dd>
                          <span aria-label="にまんよんせんえん">
                            <span class="top-menu-price">24,000</span>
                            <span class="top-menu-yen">円</span>
                          </span>
                          <span class="top-menu-unit-price">(1回あたり<span aria-label="さんぜんえん">3,000円</span>)</span>
                        </dd>
                      </dl>
                    </div><!-- /.top-menu-continuation -->
                    <small class="top-menu-note">※詳細は店頭にてご案内いたします</small>
                  </div><!-- /.top-menu-detail -->
                </div><!-- /.top-menu-item-body -->
              </li>
              <li class="top-menu-item">
                <div class="top-menu-lesson-title top-menu-lesson-title--private">
                  <LessonTitle
                  titleText="プライベートレッスン"
                  titleTextClass="c-lesson-title-text c-lesson-title-text--private"
                  imgSrc="/assets/images/global/text-private-lesson.svg"
                  imgWidth="299"
                  imgHeight="100"
                  imgClass="c-lesson-title-image c-lesson-title-image--02"
                  />
                </div><!-- /.top-menu-lesson-title -->
                <div class="top-menu-item-header">
                  <div class="top-menu-description">
                    <p class="top-menu-text">誰にも気兼ねせず、<br>自分のペースで進められる特別なレッスン。</p>
                    <p class="top-menu-text">心と体の状態に丁寧に向き合いながら、<br>一人ひとりに合わせて<br>必要なサポートをご提供します。</p>
                    <p class="top-menu-text">"自分だけの時間”で、忙しい日常を少し離れて、<br>心地よいリズムを取り戻しませんか?</p>
                  </div><!-- /.top-menu-description -->
                  <div class="top-menu-image">
                    <img src="/assets/images/menu/pic-menu-private@2x.jpg" alt="" loading="lazy" decoding="async" width="570" height="330">
                  </div><!-- /.top-menu-image -->
                </div><!-- /.top-menu-item-header -->
                <div class="top-menu-item-body">
                  <div class="top-menu-detail">
                    <h4 class="top-menu-detail-heading">単発で通いたい方</h4>
                    <div class="top-menu-one-off">
                      <dl class="top-menu-one-off-def-list">
                        <dt>カウンセリング<span class="top-menu-first-time">(初回のみ)</span><br><span class="top-menu-plus">＋</span>姿勢チェック<span class="top-menu-plus">＋</span>50分レッスン</dt>
                        <dd>
                          <span aria-label="ななせんごひゃくえん">
                            <span class="top-menu-price">7,500</span>
                            <span class="top-menu-yen">円</span>
                          </span>
                        </dd>
                      </dl>
                    </div><!-- /.top-menu-one-off -->
                  </div><!-- /.top-menu-detail -->
                  <div class="top-menu-detail">
                    <h4 class="top-menu-detail-heading top-menu-detail-heading--bg-skyblue">継続して通いたい方</h4>
                    <div class="top-menu-continuation">
                      <p class="top-menu-coupon-ticket">回数券</p>
                      <dl class="top-menu-continuation-def-list">
                        <dt>4回券</dt>
                        <dd>
                          <span aria-label="にまんはっせんはっぴゃくえん">
                            <span class="top-menu-price">28,800</span>
                            <span class="top-menu-yen">円</span>
                          </span>
                          <span class="top-menu-unit-price">(1回あたり<span aria-label="ななせんにひゃくえん">7,200円</span>)</span>
                        </dd>
                        <dt>8回券</dt>
                        <dd>
                          <span aria-label="ごまんろくせんえん">
                            <span class="top-menu-price">56,000</span>
                            <span class="top-menu-yen">円</span>
                          </span>
                          <span class="top-menu-unit-price">(1回あたり<span aria-label="ななせんえん">7,000円</span>)</span>
                        </dd>
                      </dl>
                    </div><!-- /.top-menu-continuation -->
                    <small class="top-menu-note">※詳細は店頭にてご案内いたします</small>
                  </div><!-- /.top-menu-detail -->
                </div><!-- /.top-menu-item-body -->
              </li>
            </ul>
          </div><!-- /.top-menu-section-body.l-section-body -->
        </div><!-- l-container-s -->
      </section><!-- /.top-menu -->

      <!-- top-flow -->
      <section class="top-flow">
        <div class="l-container-s">
          <h2 class="top-flow-title">おすすめの流れ</h2>
          <div class="top-flow-section-body l-section-body">
            <ol class="top-flow-list">
              <li class="top-flow-item">
                <div class="top-flow-description">
                  <div class="top-flow-heading">
                    <div class="top-flow-heading-num">
                      <img src="/assets/images/flow/text-flow-01.svg" alt="01" loading="lazy" decoding="async" width="105" height="119">
